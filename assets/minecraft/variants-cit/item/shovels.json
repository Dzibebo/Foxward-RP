{
  "type": "component_data",
  "items": [
    "minecraft:wooden_shovel",
    "minecraft:stone_shovel",
    "minecraft:iron_shovel",
    "minecraft:golden_shovel",
    "minecraft:diamond_shovel",
    "minecraft:netherite_shovel"
  ],
  "modelPrefix": "item/shovels/",
  "modelParent": "minecraft:item/handheld",
  "parameters": {
    "componentType": "custom_name",
    "expect": "rich_text",
    "debug": true,
    "transform": ["lowercase",
      {
        "regex": "(?iu)ё",
        "substitution": "е",
        "matchAll": false,
        "validate": false
      },{
        "regex": "(?iu)темная лопата",
        "substitution": "dark_shovel",
        "matchAll": true,
        "validate": false
      },

      {
        "regex": "(?iu)отбойный молоток",
        "substitution": "jackhammer",
        "matchAll": true,
        "validate": false
      },

      {
        "regex": "(?iu)металлическая лопата",
        "substitution": "metal_shovel",
        "matchAll": true,
        "validate": false
      },
      {
        "regex": "(?iu)лопата из луностали",
        "substitution": "moonsteel_shovel",
        "matchAll": true,
        "validate": false
      },
      {
        "regex": "(?iu)пластиковая лопатка",
        "substitution": "plastic",
        "matchAll": true,
        "validate": false
      },
      {
        "regex": "(?iu)пластиковая ложка",
        "substitution": "plastic_spoon",
        "matchAll": true,
        "validate": false
      },
      {
        "regex": "(?iu)снегоуборочная лопата",
        "substitution": "snow_shovel",
        "matchAll": true,
        "validate": false
      },
      {
        "regex": "(?iu)штыковая лопата",
        "substitution": "spade_shovel",
        "matchAll": true,
        "validate": false
      },
      {
        "regex": "(?iu)шпатель",
        "substitution": "spatula",
        "matchAll": true,
        "validate": false
      },
      {
        "regex": "(?iu)ложка",
        "substitution": "spoon",
        "matchAll": true,
        "validate": false
      },
      {
        "regex": "(?iu)совковая лопата",
        "substitution": "square_shovel",
        "matchAll": true,
        "validate": false
      },
      {
        "regex": "(?iu)траншейная лопата",
        "substitution": "trench_shovel",
        "matchAll": true,
        "validate": false
      },
      {
        "regex": "(?iu)мастерок",
        "substitution": "trowel",
        "matchAll": true,
        "validate": false
      }
    ]
  }
}