{
  "type": "component_data",
  "items": [
    "minecraft:ominous_trial_key",
    "minecraft:trial_key"
  ],
  "modelPrefix": "item/trial_key/",
  "modelParent": "minecraft:item/generated",
  "parameters": {
    "componentType": "item_name",
    "expect": "rich_text",
    "debug": true,
    "transform": ["lowercase",
      {
        "regex": "(?iu)ключ испытаний 3d",
        "substitution": "key",
        "matchAll": true,
        "validate": false
      },
      {
        "regex": "(?iu)зловещий ключ испытаний 3d",
        "substitution": "ominous_key",
        "matchAll": true,
        "validate": false
      }
    ]
  }
}